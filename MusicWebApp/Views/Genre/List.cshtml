        <div class="thumbnail">
            <img src="@genre.ImageLocation" style="height: 100px; width: auto;" alt=""/>
            @*<div class="caption">
                <h3>
                    <a>@genre.Title</a>
                </h3>
            </div>*@
        </div>

    </div>
}